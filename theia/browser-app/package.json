{
  "private": true,
  "name": "ivy-browser-app",
  "version": "0.1.0",
  "dependencies": {
    "@theia/application-package": "1.4.0",
    "@theia/callhierarchy": "1.4.0",
    "@theia/console": "1.4.0",
    "@theia/core": "1.4.0",
    "@theia/debug": "1.4.0",
    "@theia/editor": "1.4.0",
    "@theia/file-search": "1.4.0",
    "@theia/filesystem": "1.4.0",
    "@theia/languages": "1.4.0",
    "@theia/markers": "1.4.0",
    "@theia/messages": "1.4.0",
    "@theia/monaco": "1.4.0",
    "@theia/navigator": "1.4.0",
    "@theia/output": "1.4.0",
    "@theia/plugin-dev": "1.4.0",
    "@theia/plugin-ext": "1.4.0",
    "@theia/plugin-ext-vscode": "1.4.0",
    "@theia/preferences": "1.4.0",
    "@theia/process": "1.4.0",
    "@theia/scm": "1.4.0",
    "@theia/search-in-workspace": "1.4.0",
    "@theia/task": "1.4.0",
    "@theia/terminal": "1.4.0",
    "@theia/userstorage": "1.4.0",
    "@theia/variable-resolver": "1.4.0",
    "@theia/workspace": "1.4.0",
    "@ivy-glsp/ivy-glsp-theia": "0.8.0"
  },
  "devDependencies": {
    "@theia/cli": "1.4.0"
  },
  "theiaPluginsDir": "../plugins",
  "scripts": {
    "prepare": "theia build --mode development",
    "start": "theia start --IVY_PROCESS_GLSP=5007 --plugins=local-dir:../plugins",
    "start:debug": "theia start --IVY_PROCESS_GLSP=5007 --plugins=local-dir:../plugins --loglevel=debug --DEBUG",
    "watch": "theia build --watch --mode development"
  },
  "theia": {
    "target": "browser"
  }
}
