{
  "name": "@ivy-glsp/ivy-glsp-theia",
  "version": "0.8.0",
  "keywords": [
    "theia-extension"
  ],
  "description": "Theia extension for the Ivy GLSP client",
  "license": "(EPL-2.0 OR GPL-2.0 WITH Classpath-exception-2.0)",
  "files": [
    "lib",
    "src"
  ],
  "author": {
    "name": "Eclipse GLSP"
  },
  "dependencies": {
    "@ivy-glsp/ivy-glsp-client": "0.8.0",
    "@eclipse-glsp/theia-integration": "0.8.0",
    "@theia/debug": "1.4.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.56.0",
    "rimraf": "^2.6.1",
    "typescript": "3.9.2"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "build": "tsc && yarn run lint",
