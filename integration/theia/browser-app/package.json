{
  "private": true,
  "name": "@ivyteam/theia-browser-app",
  "version": "0.9.3-s14",
  "description": "Theia IDE with integrated GLSP-based Axon Ivy process editor",
  "dependencies": {
    "@ivyteam/theia-integration": "^0.9.3-s14",
    "@theia/core": "^1.0.0",
    "@theia/editor": "^1.0.0",
    "@theia/filesystem": "^1.0.0",
    "@theia/markers": "^1.0.0",
    "@theia/messages": "^1.0.0",
    "@theia/monaco": "^1.0.0",
    "@theia/navigator": "^1.0.0",
    "@theia/preferences": "^1.0.0",
    "@theia/process": "^1.0.0",
    "@theia/terminal": "^1.0.0",
    "@theia/workspace": "^1.0.0"
  },
  "devDependencies": {
    "@theia/cli": "^1.0.0"
  },
  "scripts": {
    "prepare": "yarn build",
    "build": "theia build --mode development",
    "start": "theia start --IVY_PROCESS_GLSP=5007",
    "start:debug": "theia start --IVY_PROCESS_GLSP=5007 --loglevel=debug --debug",
    "watch": "theia build --watch --mode development"
  },
  "theia": {
    "target": "browser"
  }
}
