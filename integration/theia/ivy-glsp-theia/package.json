{
  "name": "@ivyteam/theia-integration",
  "version": "0.9.0",
  "keywords": [
    "theia-extension"
  ],
  "description": "Theia extension for the Ivy GLSP client",
  "license": "(EPL-2.0 OR GPL-2.0 WITH Classpath-exception-2.0)",
  "files": [
    "lib",
    "src"
  ],
  "author": {
    "name": "Eclipse GLSP"
  },
  "dependencies": {
    "@ivyteam/process-editor": "0.9.0-1",
    "@eclipse-glsp/theia-integration": "next"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "ts-node": "9.0.0",
    "typescript": "3.9.2"
  },
  "scripts": {
    "prepare": "yarn clean && yarn build && yarn lint",
    "clean": "rimraf lib",
    "build": "tsc",
    "lint": "eslint -c ./.eslintrc.js --ext .ts,.tsx ./src",
    "watch": "tsc -w"
