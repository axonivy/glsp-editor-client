{
  "private": true,
  "name": "parent",
  "version": "10.0.1",
  "engines": {
    "node": ">=20.10.0"
  },
  "scripts": {
    "ci": "npm install && npm run build:icons && npm run build && npm run type && npm run package",
    "clean": "lerna run clean",
    "build": "tsc --build && tscp --build",
    "build:icons": "npm run -w @ivyteam/process-editor build:icons",
    "package": "lerna run package",
    "type": "lerna run type",
    "lint": "lerna run lint --",
    "lint:ci": "npm run lint -- -o eslint.xml -f checkstyle",
    "dev": "npm run -w @ivyteam/standalone-integration dev",
    "dev:eclipse": "npm run -w @ivyteam/eclipse-integration dev",
    "test": "npm run -w @ivyteam/process-editor test",
    "test:ci": "npm run -w @ivyteam/process-editor test:ci",
    "webtest": "npm run -w @ivyteam/standalone-integration webtest",
    "publish:package": "lerna publish from-package --exact --dist-tag=sprint --yes"
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^8.57.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-no-null": "^1.0.2",
    "lerna": "^8.2.0",
    "prettier": "^3.5.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "typescript-cp": "^0.1.9",
    "vite": "^6.1.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vite-plugin-node-polyfills": "^0.23.0",
    "setimmediate": "^1.0.5"
  },
  "workspaces": {
    "packages": [
      "editor",
      "integration/standalone",
      "integration/eclipse"
    ]
  },
  "volta": {
    "node": "22.11.0"
  }
}
